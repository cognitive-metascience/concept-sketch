      "relation_type": "DEP"
    },
    {
      "id": "dep_nmod",
      "name": "Dependency: nominal modifier",
      "description": "Noun with its nominal modifier (e.g., 'government policy')",
      "pattern": "1:[xpos=\"NN.*\"] 2:[deprel=\"nmod\"]",
      "default_slop": 0,
      "relation_type": "DEP"
    },
    {
      "id": "dep_compound",
      "name": "Dependency: compound",
      "description": "Noun in compound with head noun (e.g., 'coffee house')",
      "pattern": "1:[xpos=\"NN.*\"] 2:[deprel=\"compound\"]",
      "default_slop": 0,
      "relation_type": "DEP"
    },
    {
      "id": "dep_advmod",
      "name": "Dependency: adverbial modifier",
