{
  "version": "2.0",
  "description": "BCQL grammar - positions derived from numbered labels (1: = head, 2: = collocate)",
  "bcql": true,
  "relations": [
    {
      "id": "noun_adj_predicates",
      "name": "Adjectives (predicative)",
      "description": "Adjective predicates with copula (e.g., 'hypothesis is valid', 'theory appears correct')",
      "pattern": "1:[xpos=\"NN.*\"] [lemma=\"be|appear|seem|look|sound|feel|smell|taste|remain|become|get|grow|turn|prove\"] 2:[xpos=\"JJ.*\"]",
      "relation_type": "SURFACE",
      "dual": false
    },
    {
      "id": "noun_modifiers",
      "name": "Modifiers (adjectives)",
      "description": "Adjectives modifying nouns (e.g., 'big house', 'red car')",
      "pattern": "2:[xpos=\"JJ.*\"] 1:[xpos=\"NN.*\"]",
      "relation_type": "SURFACE",
      "dual": false
    },
    {
      "id": "subject_of",
      "name": "Subjects",
      "description": "Subject of verbs",
      "pattern": "1:[xpos=\"NN.*\"] 2:[xpos=\"VB.*\"]",
      "relation_type": "SURFACE"
    },
    {
      "id": "object_of",
      "name": "Objects (direct)",
      "description": "Direct objects of verbs",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"NN.*\"]",
      "relation_type": "SURFACE"
    },
    {
      "id": "noun_verbs",
      "name": "Verbs (subject of)",
      "description": "Verbs appearing near nouns (subjects)",
      "pattern": "1:[xpos=\"NN.*\"] []{0,3} 2:[xpos=\"VB.*\"]",
      "relation_type": "SURFACE"
    },
    {
      "id": "noun_compounds",
      "name": "Nouns in compound",
      "description": "Other nouns in compound (e.g., 'coffee house', 'house party')",
      "pattern": "1:[xpos=\"NN.*\"] 2:[xpos=\"NN.*\"]",
      "relation_type": "SURFACE"
    },
    {
      "id": "noun_prepositions",
      "name": "Prepositions",
      "description": "Prepositions (e.g., 'house of', 'at the house')",
      "pattern": "1:[xpos=\"NN.*\"] 2:[xpos=\"IN\"]",
      "relation_type": "SURFACE"
    },
    {
      "id": "noun_adverbs",
      "name": "Adverbs",
      "description": "Adverbs modifying nouns (e.g., 'almost home')",
      "pattern": "2:[xpos=\"RB.*\"] 1:[xpos=\"NN.*\"]",
      "relation_type": "SURFACE"
    },
    {
      "id": "noun_possessives",
      "name": "Possessive nouns",
      "description": "Possessive forms (e.g., 'house's')",
      "pattern": "2:[xpos=\"NN.*Z\"] 1:[xpos=\"NN.*\"]",
      "default_slop": 1,
      "relation_type": "SURFACE"
    },
    {
      "id": "predicate_of",
      "name": "Predicate nouns",
      "description": "Noun predicates with 'be' (e.g., 'theory is a science', 'this is evidence')",
      "pattern": "1:[xpos=\"NN.*\"] [lemma=\"be|appear|seem|look|sound|feel|smell|taste|remain|become|get|grow|turn|prove\"] 2:[xpos=\"NN.*\"]",
      "default_slop": 8,
      "relation_type": "SURFACE"
    },
    {
      "id": "pro_object",
      "name": "Pronominal objects",
      "description": "Pronouns as objects (e.g., 'tell them', 'see us')",
      "pattern": "2:[xpos=\"VB.*\"] 1:[xpos=\"PRP\"]",
      "default_slop": 3,
      "relation_type": "SURFACE"
    },
    {
      "id": "pro_subject",
      "name": "Pronominal subjects",
      "description": "Pronouns as subjects (e.g., 'it works', 'they agree')",
      "pattern": "2:[xpos=\"PRP\"] 1:[xpos=\"VB.*\"]",
      "default_slop": 3,
      "relation_type": "SURFACE"
    },
    {
      "id": "modifier_n",
      "name": "Noun modifiers (of nouns)",
      "description": "Nouns modifying nouns (e.g., 'city government', 'cancer research')",
      "pattern": "2:[xpos=\"NN.*\"] 1:[xpos=\"NN.*\"]",
      "default_slop": 2,
      "relation_type": "SURFACE"
    },
    {
      "id": "modifier_j",
      "name": "Adjective modifiers (of adjectives)",
      "description": "Adverbs modifying adjectives (e.g., 'very happy', 'extremely difficult')",
      "pattern": "2:[xpos=\"RB.*\"] 1:[xpos=\"JJ.*\"]",
      "default_slop": 1,
      "relation_type": "SURFACE"
    },
    {
      "id": "modifier_v",
      "name": "Verb modifiers",
      "description": "Adverbs modifying verbs (e.g., 'quickly run', 'carefully analyze')",
      "pattern": "2:[xpos=\"RB.*\"] 1:[xpos=\"VB.*\"]",
      "default_slop": 2,
      "relation_type": "SURFACE"
    },
    {
      "id": "possessor",
      "name": "Possessors",
      "description": "Possessive nouns (e.g., 'John's house', 'government's policy')",
      "pattern": "2:[xpos=\"NN.*Z\"] 1:[xpos=\"NN.*\"]",
      "default_slop": 1,
      "relation_type": "SURFACE"
    },
    {
      "id": "pro_possessor",
      "name": "Pronominal possessors",
      "description": "Possessive pronouns (e.g., 'my house', 'their policy')",
      "pattern": "2:[xpos=\"PP$\"] 1:[xpos=\"NN.*\"]",
      "default_slop": 1,
      "relation_type": "SURFACE"
    },
    {
      "id": "wh_comp",
      "name": "Wh-words",
      "description": "Wh-words following verbs (e.g., 'know what', 'ask who')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"W.*\"]",
      "default_slop": 3,
      "relation_type": "SURFACE"
    },
    {
      "id": "infincomp",
      "name": "Infinitive complements",
      "description": "Infinitive objects (e.g., 'want to go', 'need to know')",
      "pattern": "1:[xpos=\"VB.*\"] [xpos=\"TO\"] 2:[xpos=\"VB.*\"]",
      "default_slop": 4,
      "relation_type": "SURFACE"
    },
    {
      "id": "ing_comp",
      "name": "-ing complements",
      "description": "Gerund complements (e.g., 'enjoy reading', 'consider leaving')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"VBG\"]",
      "default_slop": 3,
      "relation_type": "SURFACE"
    },
    {
      "id": "passive",
      "name": "Passive constructions",
      "description": "Passive voice (e.g., 'is known by', 'was developed by')",
      "pattern": "1:[xpos=\"VBN\"] [word=\"by\"] 2:[xpos=\"NN.*\"]",
      "default_slop": 8,
      "relation_type": "SURFACE"
    },
    {
      "id": "reflexive",
      "name": "Reflexive constructions",
      "description": "Reflexive pronouns (e.g., 'protect themselves', 'enjoy himself')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"PRP\" & word=\".*self|.*selves\"]",
      "default_slop": 3,
      "relation_type": "SURFACE"
    },
    {
      "id": "it_cleft",
      "name": "It-cleft constructions",
      "description": "It's X to... patterns (e.g., 'it's important to note')",
      "pattern": "2:[word=\"it\"] [xpos=\"VB.*\"] 1:[xpos=\"JJ.*\"]",
      "default_slop": 10,
      "relation_type": "SURFACE"
    },
    {
      "id": "prep_phrase",
      "name": "Prepositional phrases",
      "description": "Prepositional phrases (e.g., 'house in London', 'book on table')",
      "pattern": "1:[xpos=\"NN.*\"] [xpos=\"IN\"] 2:[xpos=\"NN.*\"]",
      "default_slop": 5,
      "relation_type": "SURFACE"
    },
    {
      "id": "np_adj_comp",
      "name": "Adjective complements",
      "description": "Adjectives as complements of verbs (e.g., 'consider important', 'keep clean')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"JJ.*\"]",
      "default_slop": 4,
      "relation_type": "SURFACE"
    },
    {
      "id": "adj_comp",
      "name": "Adjectives (complements)",
      "description": "Adjectives after verbs (e.g., 'becomes happy', 'seems difficult')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"JJ.*\"]",
      "default_slop": 4,
      "relation_type": "SURFACE"
    },
    {
      "id": "part_intrans",
      "name": "Intransitive particles",
      "description": "Particles after intransitive verbs (e.g., 'wake up', 'break down')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"RP\"]",
      "default_slop": 1,
      "relation_type": "SURFACE"
    },
    {
      "id": "part_trans",
      "name": "Transitive particles",
      "description": "Particles with transitive verbs (e.g., 'pick up', 'put down')",
      "pattern": "1:[xpos=\"VB.*\"] [xpos=\"NN.*\"] 2:[xpos=\"RP\"]",
      "default_slop": 2,
      "relation_type": "SURFACE"
    },
    {
      "id": "verb_nouns",
      "name": "Nouns (objects/subjects)",
      "description": "Nouns appearing near verbs (objects/subjects)",
      "pattern": "1:[xpos=\"NN.*\"]",
      "dual": true,
      "relation_type": "SURFACE"
    },
    {
      "id": "verb_particles",
      "name": "Particles",
      "description": "Particles (e.g., 'give up', 'break down')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"RP\"]",
      "default_slop": 2,
      "relation_type": "SURFACE"
    },
    {
      "id": "verb_prepositions",
      "name": "Prepositions",
      "description": "Prepositions (e.g., 'depend on', 'look at')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"IN\"]",
      "default_slop": 5,
      "relation_type": "SURFACE"
    },
    {
      "id": "verb_adverbs",
      "name": "Adverbs",
      "description": "Adverbs modifying verbs",
      "pattern": "2:[xpos=\"RB.*\"] 1:[xpos=\"VB.*\"]",
      "default_slop": 3,
      "relation_type": "SURFACE"
    },
    {
      "id": "verb_adjectives",
      "name": "Adjectives (verb context)",
      "description": "Adjectives in verb context (e.g., 'become happy')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"JJ.*\"]",
      "default_slop": 4,
      "relation_type": "SURFACE"
    },
    {
      "id": "verb_to",
      "name": "Infinitive 'to'",
      "description": "Infinitive marker (e.g., 'want to', 'need to')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"TO\"]",
      "default_slop": 2,
      "relation_type": "SURFACE"
    },
    {
      "id": "verb_verbs",
      "name": "Other verbs",
      "description": "Verb chains (e.g., 'try to make')",
      "pattern": "1:[xpos=\"VB.*\"] [xpos=\"TO\"] 2:[xpos=\"VB.*\"]",
      "default_slop": 5,
      "relation_type": "SURFACE"
    },
    {
      "id": "adj_nouns",
      "name": "Nouns modified",
      "description": "Nouns modified by the adjective",
      "pattern": "2:[xpos=\"JJ.*\"] 1:[xpos=\"NN.*\"]",
      "default_slop": 3,
      "relation_type": "SURFACE"
    },
    {
      "id": "adj_adverbs",
      "name": "Adverbs (intensifiers)",
      "description": "Adverbs modifying adjectives (e.g., 'very big')",
      "pattern": "2:[xpos=\"RB.*\"] 1:[xpos=\"JJ.*\"]",
      "default_slop": 3,
      "relation_type": "SURFACE"
    },
    {
      "id": "adj_verbs",
      "name": "Verbs",
      "description": "Verbs (e.g., 'be happy', 'become sad')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[xpos=\"JJ.*\"]",
      "default_slop": 4,
      "relation_type": "SURFACE"
    },
    {
      "id": "adj_prepositions",
      "name": "Prepositions",
      "description": "Prepositions (e.g., 'afraid of', 'good at')",
      "pattern": "1:[xpos=\"JJ.*\"] 2:[xpos=\"IN\"]",
      "default_slop": 5,
      "relation_type": "SURFACE"
    },
    {
      "id": "adj_coordinated",
      "name": "Coordinated adjectives (and/or)",
      "description": "Adjectives in coordination (e.g., 'big and red')",
      "pattern": "1:[xpos=\"JJ.*\"] [lemma=\"and|or\"] 2:[xpos=\"JJ.*\"]",
      "default_slop": 2,
      "relation_type": "SURFACE"
    },
    {
      "id": "dep_nsubj",
      "name": "Dependency: nominal subject",
      "description": "Verb with its nominal subject (e.g., 'theory explains')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[deprel=\"nsubj\"]",
      "default_slop": 0,
      "relation_type": "DEP"
    },
    {
      "id": "dep_nsubj_pass",
      "name": "Dependency: passive nominal subject",
      "description": "Verb with its passive nominal subject (e.g., 'theory is studied')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[deprel=\"nsubj:pass\"]",
      "default_slop": 0,
      "relation_type": "DEP"
    },
    {
      "id": "dep_obj",
      "name": "Dependency: direct object",
      "description": "Verb with its direct object (e.g., 'explain theory')",
      "pattern": "1:[xpos=\"VB.*\"] 2:[deprel=\"obj\"]",
      "default_slop": 0,
